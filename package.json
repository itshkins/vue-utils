{
  "name": "@itshkins/vue-utils",
  "type": "module",
  "version": "1.0.0",
  "description": "Vue composables",
  "main": "./dist/vue-utils.cjs",
  "browser": "./dist/vue-utils.js",
  "types": "./dist/vue-utils.d.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "start": "npm run clean && NODE_ENV=development rollup -c -w",
    "build": "npm run clean && NODE_ENV=production rollup -c",
    "test": "npm run test:unit",
    "test:unit": "vitest --root src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itshkins/vue-utils.git"
  },
  "keywords": [
    "vue",
    "composables",
    "directives"
  ],
  "author": "itshkins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itshkins/vue-utils/issues"
  },
  "homepage": "https://github.com/itshkins/vue-utils#readme",
  "dependencies": {
    "vue": "^2.6",
    "vue-demi": "0.14.5"
  },
  "devDependencies": {
    "@vue/test-utils": "1.3.6",
    "jsdom": "22.1.0",
    "rollup": "3.26.2",
    "rollup-plugin-dts": "5.3.0",
    "rollup-plugin-esbuild": "5.0.0",
    "vitest": "0.33.0"
  }
}
